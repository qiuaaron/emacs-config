class ${1:driver_name} extends uvm_sequence #( ${2:item_name} );
  \`uvm_object_utils_begin($1)
  \`uvm_object_utils_end($1)
  function new ( string name = "$1" );
    super.new( name );
    $0
  endfunction // new

  task body( );
  endtask // body

endclass // $1
